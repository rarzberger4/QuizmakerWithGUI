<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.quizmakerwithgui.GController">
   <children>
      <GridPane alignment="CENTER" layoutX="301.0" layoutY="503.0" prefHeight="87.0" prefWidth="347.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="342.0" minWidth="10.0" prefWidth="295.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="164.0" minWidth="5.0" prefWidth="52.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="93.4" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="87.4" minHeight="10.0" prefHeight="37.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="88.4" minHeight="10.0" prefHeight="39.599999999999994" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="buttonC" mnemonicParsing="false" onAction="#onButtonCclick" text="C" GridPane.rowIndex="2" />
            <Button fx:id="buttonD" alignment="CENTER" contentDisplay="RIGHT" mnemonicParsing="false" onAction="#onButtonDclick" prefHeight="25.0" prefWidth="37.0" text="D" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Button fx:id="buttonA" mnemonicParsing="false" onAction="#onButtonAclick" text="A" />
            <Button fx:id="buttonB" mnemonicParsing="false" onAction="#onButtonBclick" text="B" GridPane.columnIndex="1" />
         </children>
      </GridPane>
      <Label alignment="CENTER" contentDisplay="TOP" layoutX="368.0" prefHeight="96.0" prefWidth="211.0" text="Game-Beta" AnchorPane.topAnchor="0.0">
         <font>
            <Font name="SansSerif Bold" size="34.0" />
         </font>
      </Label>
      <TextArea fx:id="GameTextArea" layoutX="143.0" layoutY="65.0" onMouseClicked="#startGame" prefHeight="422.0" prefWidth="676.0" />
      <Button fx:id="quitbutton" layoutX="474.0" layoutY="751.0" mnemonicParsing="false" onAction="#onquitbuttonClick" text="Quit" />
      <Button fx:id="buttonSkip" layoutX="436.0" layoutY="655.0" mnemonicParsing="false" onAction="#onButtonSkipclick" text="Skip" />
      <Button fx:id="button5050" layoutX="500.0" layoutY="655.0" mnemonicParsing="false" onAction="#onButton5050click" text="50/50" />
   </children>
</AnchorPane>
